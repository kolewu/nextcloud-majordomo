{
  "name": "majordomo",
  "version": "1.0.0",
  "description": "Place this app in **nextcloud/apps/**",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dist": "npm run build && npm run appstore",
    "build": "NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.js",
    "appstore": "tar --exclude-ignore=.appignore --transform 's,^\\./,majordomo/,' -cvzf majordomo.tar.gz .",
    "dev": "NODE_ENV=development webpack --progress --config webpack.dev.js",
    "watch": "NODE_ENV=development webpack --progress --watch --config webpack.dev.js"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@bundle-analyzer/webpack-plugin": "^0.5.1",
    "@nextcloud/l10n": "^1.2.0",
    "@nextcloud/vue": "^1.4.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@nextcloud/browserslist-config": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^5.16.0",
    "eslint-config-nextcloud": "0.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-nextcloud": "^0.3.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.3.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.16.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
