{
  "name": "majordomo",
  "version": "1.0.0",
  "description": "Place this app in **nextcloud/apps/**",
  "main": "index.js",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dist": "npm run build && npm run appstore",
    "build": "NODE_ENV=production webpack --progress --config webpack.prod.js",
    "appstore": "touch majordomo.tar.gz && tar --exclude-ignore=.appignore --transform 's,^\\./,majordomo/,' -cvzf majordomo.tar.gz .",
    "dev": "NODE_ENV=development webpack --progress --config webpack.dev.js",
    "watch": "NODE_ENV=development webpack --progress --watch --config webpack.dev.js"
  },
  "author": "",
  "license": "UNLICENSED",
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.2",
    "@bundle-analyzer/webpack-plugin": "^0.5.1",
    "@nextcloud/axios": "^1.6.0",
    "@nextcloud/browserslist-config": "^2.1.0",
    "@nextcloud/eslint-config": "^5.1.0",
    "@nextcloud/eslint-plugin": "^2.0.0",
    "@nextcloud/l10n": "^1.4.1",
    "@nextcloud/router": "^2.0.0",
    "@nextcloud/vue": "^3.9.0",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "node-sass": "^6.0.0",
    "sass-loader": "^11.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.7",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  }
}
